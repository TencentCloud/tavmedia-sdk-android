<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.tencent.tavmedia.demo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <uses-feature
            android:glEsVersion="0x00020000"
            android:required="true" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.TAVMediaAndroid">
        <activity
                android:name=".AutoTestActivity"
                android:exported="false" />
        <activity
                android:name=".PAGTemplateActivity"
                android:exported="false" />
        <activity
                android:name=".SerializableActivity"
                android:exported="false" />
        <activity
                android:name=".ColorTuningActivity"
                android:exported="false" />
        <activity
                android:name=".UpdateRenderSizeActivity"
                android:exported="false" />
        <activity
                android:name=".SnapshotActivity"
                android:exported="false" />
        <activity
                android:name=".MultiClipActivity"
                android:exported="false" />
        <activity
                android:name=".AudioPlayerActivity"
                android:exported="false" />
        <activity
                android:name=".TemplateActivity"
                android:exported="false" />
        <activity
                android:name=".ExportActivity"
                android:exported="false" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>